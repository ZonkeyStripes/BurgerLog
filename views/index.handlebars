<h2>The Burger Menu!</h2>

<ul>
  {{#each burgers}}
    {{#unless devoured}}
      
    <li>
        {{burger_name}}
        <button class="burgerEat" data-id="{{this.id}}"></button>
    </li>

    {{/unless}}
  {{/each}}
</ul>

<form class="create-form">

  <div class="form-group">
    <label for="ca">Add a Burger!:</label>
    <input type="text" id="burg" name="name">
  </div>